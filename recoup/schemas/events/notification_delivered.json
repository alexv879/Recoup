{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "notification_delivered",
    "type": "object",
    "additionalProperties": false,
    "required": [
        "notification_id",
        "type"
    ],
    "properties": {
        "notification_id": {
            "type": "string",
            "minLength": 6
        },
        "type": {
            "type": "string",
            "enum": [
                "system",
                "behavioral",
                "escalation"
            ]
        },
        "timestamp": {
            "type": "string",
            "format": "date-time"
        }
    }
}