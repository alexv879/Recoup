{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "experiment_exposed",
    "type": "object",
    "additionalProperties": false,
    "required": [
        "experiment_key",
        "variant_key",
        "context"
    ],
    "properties": {
        "experiment_key": {
            "type": "string",
            "minLength": 3
        },
        "variant_key": {
            "type": "string",
            "minLength": 1
        },
        "context": {
            "type": "string",
            "minLength": 2
        },
        "timestamp": {
            "type": "string",
            "format": "date-time"
        }
    }
}